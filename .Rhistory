ds<-function(x){
#1 row and 2 columns
par(mfrow = c(1,2))
#histogram
hist(x,co = rainbow(30))
#box plot
boxplot(x,col ='green' )
par(mfrow =c(1,1))
#numeric summary
data.frame(min = max(x), median =median(x),mean =mean(x),max = max(x))
}
ds(3)
